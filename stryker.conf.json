{
    "$schema": "./node_modules/@stryker-mutator/core/schema/stryker-schema.json",
    "mutator": "typescript",
    "packageManager": "npm",
    "reporters": [
      "html",
      "clear-text",
      "progress"
    ],
    "testRunner": "jest",
    "coverageAnalysis": "off",
    "timeoutMS": 10000,
    "maxConcurrentTestRunners": 2,
    "thresholds": {
      "high": 100,
      "low": 80,
      "break": 75
    },
    "tsconfigFile": "tsconfig.json",
    "tempDirName": "stryker-tmp"
  }